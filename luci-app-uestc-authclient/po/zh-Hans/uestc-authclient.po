# Translations for LuCI interface
msgid "UESTC Authentication Client"
msgstr "UESTC 清水河认证客户端"

msgid "This page is used to configure the UESTC Authentication Client. Please enter your username and password, and adjust other settings as needed."
msgstr "该页面用于配置 UESTC 清水河认证客户端。请填写您的用户名和密码，并根据需要调整其他设置。"

msgid "Settings"
msgstr "设置"

msgid "Enable on Startup"
msgstr "开机自动运行"

msgid "Check to run the service automatically when the system starts."
msgstr "勾选后，服务将在系统启动时自动运行。"

msgid "Authentication Method"
msgstr "认证方式"

msgid "Select the authentication method. Use Srun for new dormitories and teaching areas."
msgstr "选择认证方式，新建宿舍楼及教学区使用 Srun 认证方式"

msgid "Telecom Authentication (uestc_ct_authclient)"
msgstr "电信认证方式 (uestc_ct_authclient)"

msgid "Srun Authentication (uestc_srun_authclient)"
msgstr "Srun 认证方式 (uestc_srun_authclient)"

msgid "Telecom Username"
msgstr "电信认证方式用户名"

msgid "Your username for Telecom authentication."
msgstr "您的电信认证方式用户名。"

msgid "Required"
msgstr "必填"

msgid "Username cannot be empty."
msgstr "用户名不能为空。"

msgid "Telecom Password"
msgstr "电信认证方式密码"

msgid "Your password for Telecom authentication."
msgstr "您的电信认证方式密码。"

msgid "Password cannot be empty."
msgstr "密码不能为空。"

msgid "Telecom Authentication Host"
msgstr "电信认证方式登录主机"

msgid "Authentication server address for Telecom authentication, usually no need to modify."
msgstr "电信认证方式认证服务器地址，通常无需修改。"

msgid "Srun Username"
msgstr "Srun 认证方式用户名"

msgid "Your username for Srun authentication."
msgstr "您的 Srun 认证方式用户名。"

msgid "Srun Password"
msgstr "Srun 认证方式密码"

msgid "Your password for Srun authentication."
msgstr "您的 Srun 认证方式密码。"

msgid "Srun Authentication Mode"
msgstr "Srun 认证方式"

msgid "Select the authentication mode for Srun client."
msgstr "选择 Srun 客户端的认证方式。"

msgid "Telecom"
msgstr "电信"

msgid "Campus Network"
msgstr "校园网"

msgid "Srun Authentication Host"
msgstr "Srun 认证方式登录主机"

msgid "Authentication server address for Srun authentication. Modify according to whether you are in the teaching area or dormitory area."
msgstr "Srun 认证方式认证服务器地址，根据处于教学区或宿舍区进行修改。"

msgid "Interface"
msgstr "监听接口"

msgid "Select the network interface used for authentication."
msgstr "选择用于认证的网络接口。"

msgid "Heartbeat Hosts"
msgstr "心跳检测地址"

msgid "Hosts used to check network connectivity. You can add multiple addresses."
msgstr "用于检测网络连通性的主机地址，可以添加多个地址。"

msgid "Heartbeat Interval (seconds)"
msgstr "心跳检测间隔（秒）"

msgid "Time interval to check network status, in seconds."
msgstr "检测网络状态的时间间隔，单位为秒。"

msgid "Log Retention Days"
msgstr "日志保留天数"

msgid "Specify the number of days to retain log files. Logs exceeding the days will be cleared."
msgstr "指定日志文件的保留天数，超过天数的日志将被清除。"

msgid "Enable Scheduled Disconnect"
msgstr "启用定时断网"

msgid "Check to disconnect the network during the specified time period."
msgstr "勾选后，系统将在指定时间段内断开网络连接。"

msgid "Disconnect Start Time (Hour)"
msgstr "断网开始时间（小时）"

msgid "Disconnect End Time (Hour)"
msgstr "断网结束时间（小时）"

msgid "Disconnect start time and end time cannot be the same!"
msgstr "断网开始时间和结束时间不能相同！"

msgid "Overview"
msgstr "概览"

msgid "No logs available"
msgstr "暂无日志"

msgid "Status"
msgstr "状态"

msgid "Network Status"
msgstr "网络状态"

msgid "Connected"
msgstr "已连接"

msgid "Disconnected"
msgstr "未连接"

msgid "Last Login Time"
msgstr "上次登录时间"

msgid "None"
msgstr "无"

msgid "Service Status"
msgstr "后台运行状态"

msgid "Running"
msgstr "正在运行"

msgid "Not running"
msgstr "未运行"

msgid "Control"
msgstr "控制"

msgid "Start"
msgstr "启动"

msgid "Stop"
msgstr "停止"

msgid "Logs"
msgstr "日志"

msgid "Clear Logs"
msgstr "清除日志"

msgid "This page displays the current status of the UESTC Authentication Client and provides control functions."
msgstr "该页面显示UESTC 清水河认证客户端的当前状态，并提供控制服务的功能。"
