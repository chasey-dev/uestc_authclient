# Logging
# --------------------------------------------------------------
msgid "Logging Settings"
msgstr "日志设置"

msgid "Log Viewer"
msgstr "查看日志"

msgid "Log retention days (Session)"
msgstr "日志保留天数（当前配置）"

msgid "Specify the number of days to retain session log files."
msgstr "指定当前配置的日志文件保留天数，超过此天数的日志将被清除。"

msgid "Clean Logs"
msgstr "清理日志"

msgid "Logs of session %s have been successfully cleared."
msgstr "成功删除配置 %s 的日志。"

msgid "Error clearing logs of session %s: "
msgstr "删除配置 %s 的日志时出错："

