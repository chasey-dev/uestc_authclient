msgstr "状态"

msgid "Network Status"
msgstr "网络状态"

msgid "Last Login Time"
msgstr "上次登录时间"

msgid "Service Status"
msgstr "服务状态"

msgid "Running"
msgstr "正在运行"

msgid "Control"
msgstr "控制"

