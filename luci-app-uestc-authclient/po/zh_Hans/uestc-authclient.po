msgstr "暂无日志"

msgid "Failed to load logs."
msgstr "读取日志失败。"

# --------------------------------------------------------------
# Global
# --------------------------------------------------------------
msgid "Log retention days (Global)"
msgstr "日志保留天数（全局）"

msgid "Specify the number of days to retain global log files."
msgstr "指定全局的日志文件保留天数，超过此天数的日志将被清除。"

msgid "Global logs have been successfully cleared."
msgstr "成功清空全局日志。"

msgid "Error clearing global logs: "
msgstr "清空全局日志时出错："

# --------------------------------------------------------------
# Session: add & validate
# --------------------------------------------------------------
msgid "Add new session..."
msgstr "添加新配置..."

msgid "Please enter a new session name: (avoid using reserved names like \"global\")"
msgstr "请输入新配置的名称：（请不要使用例如 \"global\" 的保留字段）"

msgid "Session name cannot be empty."
msgstr "配置名不能为空。"

msgid "This name is reserved, please choose another."
msgstr "请不要使用保留字段作为配置名。"

msgid "Only letters, numbers, \"-\" and \"_\" are allowed (1-32 chars)."
msgstr "只能使用字母、数字以及 \"-\"、\"_\"（长度 1-32 字符）。"

msgid "Session name already exists."
msgstr "已存在相同配置名。"

msgid "Create"
msgstr "创建"

msgid "Session Configuration"
msgstr "当前配置详情"

